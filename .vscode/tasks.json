{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "bun: build",
        "type": "shell",
        "command": "bun run build",
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "problemMatcher": [],
        "options": {
          "cwd": "${workspaceFolder}"
        }
      },
      {
        "label": "bun: clean",
        "type": "shell",
        "command": "bun run clean",
        "group": {
          "kind": "build",
          "isDefault": false
        },
        "problemMatcher": [],
        "options": {
          "cwd": "${workspaceFolder}"
        }
      }
    ]
  }
  